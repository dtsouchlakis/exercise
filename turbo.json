{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"],
      "env": ["BACKEND-URL"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "start": {
      "parallel": [
        "turbo run start --workspace web",
        "turbo run start --workspace api"
      ],
      "env": ["BACKEND-URL"]
    }
  }
}
